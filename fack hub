-- ค้นหาโฟลเดอร์ Missing_Planks
local folder = game.Workspace:FindFirstChild("GAME")
if folder then
    folder = folder:FindFirstChild("Bridge")
end

if folder then
    folder = folder:FindFirstChild("Missing_Planks")
