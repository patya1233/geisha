fackTab:AddButton({
	Name = "esp TekeTeke",
	Callback = function()
local folderName = "TekeTeke"
local modelName = "TekeTeke"

local folder = game.Workspace:FindFirstChild(folderName)
if folder and folder:IsA("Folder") then
    print("โฟลเดอร์ 'TekeTeke' พบใน Workspace")

    local model = folder:FindFirstChild(modelName)
    if model and model:IsA("Model") then
        print("พบ Model 'TekeTeke' ในโฟลเดอร์ 'TekeTeke'")
        
        -- ตรวจสอบว่ามี Humanoid ใน Model หรือไม่
        local humanoid = model:FindFirstChildOfClass("Humanoid")
        if humanoid then
            print("พบ Humanoid ใน Model 'TekeTeke'")

            -- สร้าง Highlight ใหม่
            local highlight = Instance.new("Highlight")
            highlight.Adornee = humanoid.Parent -- ทำให้ Highlight แสดงที่ตัว Model ของ Humanoid
            highlight.FillColor = Color3.new(255, 0, 0) -- สีเขียว (RGB: 0, 255, 0)
            highlight.Parent = model -- วาง Highlight ลงใน Model

            print("สร้าง Highlight สีเขียวให้กับ Humanoid สำเร็จ")
        else
            print("ไม่พบ Humanoid ใน Model")
        end
    else
        print("ไม่พบ Model 'TekeTeke' ในโฟลเดอร์")
    end
else
    print("ไม่พบโฟลเดอร์ 'TekeTeke' ใน Workspace")
			end
  	end  
	})

fackTab:AddButton({
	Name = "esp note",
	Callback = function()
			-- ตรวจสอบโฟลเดอร์ CodeLocations ใน Workspace
local codeLocationsFolder = workspace:FindFirstChild("CodeLocations")

-- ตรวจสอบว่าโฟลเดอร์ CodeLocations มีอยู่หรือไม่
if codeLocationsFolder then
    -- ตรวจสอบและทำการไฮไลท์ Note ทั้ง 5 อัน
    local noteCount = 0
    for _, item in pairs(codeLocationsFolder:GetChildren()) do
        if item.Name == "Note" then
            -- ทำการไฮไลท์เป็นสีม่วง (Color3.fromRGB(128, 0, 128) คือสีม่วง)
            local highlight = Instance.new("Highlight")
            highlight.Parent = item
            highlight.FillColor = Color3.fromRGB(0, 255, 0)
            highlight.OutlineColor = Color3.fromRGB(0, 255, 0)
            noteCount = noteCount + 1

            -- หากครบ 5 Note แล้วให้หยุดทำงาน
            if noteCount == 5 then
                break
            end
        end
    end
    
    -- ตรวจสอบว่าเจอ Note ครบ 5 ไฟล์หรือไม่
    if noteCount < 5 then
        print("พบ Note เพียง " .. noteCount .. " ไฟล์ใน CodeLocations")
    else
        print("ทำการไฮไลท์ Note ทั้ง 5 อันเรียบร้อยแล้ว")
    end
else
    print("ไม่พบโฟลเดอร์ CodeLocations ใน Workspace")
			end
  	end  
	})

fackTab:AddButton({
	Name = "esp axe",
	Callback = function()
-- ตรวจสอบว่ามีโฟลเดอร์ AxePosition อยู่ใน Workspace หรือไม่
local axePositionFolder = workspace:FindFirstChild("AxePosition")

if axePositionFolder then
    -- ค้นหา tool ชื่อ Axe ในโฟลเดอร์ AxePosition
    local axeTool = axePositionFolder:FindFirstChild("Axe")
    
    if axeTool and axeTool:IsA("Tool") then
        print("พบ Tool: Axe")
        
        -- ตรวจสอบว่ามี Highlight อยู่แล้วหรือไม่
        local existingHighlight = axeTool:FindFirstChildOfClass("Highlight")
        
        if not existingHighlight then
            -- ถ้ายังไม่มี Highlight ให้สร้างใหม่
            local highlight = Instance.new("Highlight")
            highlight.Parent = axeTool
            highlight.FillColor = Color3.fromRGB(128, 0, 128) -- สีม่วง
            highlight.OutlineColor = Color3.fromRGB(128, 0, 128) -- เส้นขอบสีม่วงเช่นกัน
            
            print("Highlight สีม่วงถูกเพิ่มลงใน Axe")
        else
            print("Axe มี Highlight อยู่แล้ว")
        end
    else
        print("ไม่พบ tool ชื่อ Axe ในโฟลเดอร์ AxePosition")
    end
else
    print("ไม่พบโฟลเดอร์ AxePosition ใน Workspace")
end
    end
    })

local Section = fackTab:AddSection({
	Name = "ESP Neko"
})

fackTab:AddButton({
	Name = "esp zombie",
	Callback = function()
-- ตรวจสอบว่ามีโฟลเดอร์ Monsters อยู่ใน Workspace หรือไม่
local monstersFolder = workspace:FindFirstChild("Monsters")

if monstersFolder then
    print("พบโฟลเดอร์ Monsters ใน Workspace")

    -- วนลูปตรวจสอบโมเดลในโฟลเดอร์ Monsters
    for _, model in ipairs(monstersFolder:GetChildren()) do
        if model:IsA("Model") then
            -- ตรวจสอบว่าโมเดลนั้นมี Humanoid หรือไม่
            local humanoid = model:FindFirstChild("Humanoid")
            
            if humanoid then
                print("โมเดล " .. model.Name .. " มี Humanoid")

                -- สร้าง Highlight ใหม่
                local highlight = Instance.new("Highlight")
                highlight.Parent = model
                highlight.FillTransparency = 1 -- ทำให้ด้านในโปร่งใสทั้งหมด
                highlight.OutlineColor = Color3.fromRGB(255, 0, 0) -- ตั้งค่าสีเส้นขอบเป็นสีแดง
                highlight.OutlineTransparency = 0 -- เส้นขอบไม่โปร่งใส
            else
                print("โมเดล " .. model.Name .. " ไม่มี Humanoid")
            end
        else
            print(model.Name .. " ไม่ใช่โมเดล")
        end
    end
else
    print("ไม่พบโฟลเดอร์ Monsters ใน Workspace")
end
  	end    
        })

fackTab:AddButton({
	Name = "esp key Maze",
	Callback = function()
-- กำหนดโฟลเดอร์ที่ต้องการตรวจสอบ
local cageKeyFolder = workspace:FindFirstChild("CageKeyLocations")

-- ตรวจสอบว่ามีโฟลเดอร์นี้หรือไม่
if cageKeyFolder then
    print("Found CageKeyLocations Folder!")

    -- นับจำนวน CageKey ที่พบ
    local count = 0

    -- ลูปผ่านทุกอย่างในโฟลเดอร์
    for _, item in pairs(cageKeyFolder:GetChildren()) do
        if item:IsA("Tool") and item.Name == "CageKey" then
            count = count + 1
            
            -- สร้าง Highlight และตั้งค่าสีเขียว
            local highlight = Instance.new("Highlight")
            highlight.Parent = item
            highlight.FillColor = Color3.fromRGB(0, 255, 0) -- สีเขียว
            highlight.OutlineColor = Color3.fromRGB(0, 255, 0) -- สีขอบดำ

            -- ถ้าครบ 5 อันแล้ว ให้ออกจากลูป
            if count >= 5 then
                break
            end
        end
    end

    if count == 0 then
        print("No CageKey tools found in CageKeyLocations.")
    else
        print("Added highlights to " .. count .. " CageKey tools.")
    end
else
    print("CageKeyLocations folder not found in Workspace.")
end
    end
    })

