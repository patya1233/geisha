fackTab:AddButton({
	Name = "auto get sickle",
	Callback = function()
-- ฟังก์ชันในการตรวจสอบ ProximityPrompt
local function checkProximityPrompt()
    for i = 1, 5 do
        local sickle = workspace:FindFirstChild("Sickle" .. i)
        if sickle then
            local prompt = sickle:FindFirstChildOfClass("ProximityPrompt")
            if prompt then
                -- ตั้งค่าระยะเวลาการกดค้างเป็น 0 วินาที
                prompt.HoldDuration = 0
                print("ProximityPrompt found in Sickle" .. i .. ". HoldDuration set to 0.")
            end
        end
    end
end

-- เรียกใช้ฟังก์ชัน
checkProximityPrompt()

-- สร้างฟังก์ชันเพื่อจำลองการกด ProximityPrompt
local function simulateProximityPrompt(prompt)
    if prompt then
        print("Simulating ProximityPrompt for:", prompt.Parent.Name)
        -- จำลองการเริ่มการกด ProximityPrompt
        prompt:InputHoldBegin()
        -- จำลองการหยุดการกด ProximityPrompt
        prompt:InputHoldEnd()
    end
end

-- รายการชื่อของ Sickle1 ถึง Sickle5
local sickleList = {"Sickle1", "Sickle2", "Sickle3", "Sickle4", "Sickle5"}

-- ตรวจสอบการมีอยู่ของ Sickle1 ถึง Sickle5 ใน workspace จนกว่าทั้งหมดจะถูกลบ
while true do
    local allRemoved = true -- ตัวแปรเพื่อตรวจสอบว่าทั้งหมดถูกลบแล้วหรือไม่

    -- วนลูปผ่านแต่ละ Sickle ในรายการ
    for _, sickleName in pairs(sickleList) do
        -- ค้นหาวัตถุใน workspace
        local sickle = workspace:FindFirstChild(sickleName)
        
        -- หากพบวัตถุ
        if sickle then
            allRemoved = false -- อย่างน้อยมีวัตถุหนึ่งยังอยู่
            -- ค้นหา ProximityPrompt ภายในวัตถุ
            local proximityPrompt = sickle:FindFirstChildOfClass("ProximityPrompt")
            
            -- ถ้ามี ProximityPrompt ให้เรียกฟังก์ชันจำลองการกด
            if proximityPrompt then
                simulateProximityPrompt(proximityPrompt)
                simulateProximityPrompt(prompt)
            else
                print("No ProximityPrompt found in:", sickleName)
            end
        else
            print("No object found with name:", sickleName)
        end
    end

    -- หากวัตถุทั้งหมดถูกลบออกแล้ว ให้ออกจากลูป
    if allRemoved then
        print("All Sickle objects have been removed.")
        break
    end
    
    -- รอเล็กน้อยก่อนที่จะวนลูปใหม่
    wait(0)
end

