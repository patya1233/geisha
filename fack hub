local Section = fackTab:AddSection({
	Name = "key cage"
})

fackTab:AddButton({
	Name = "tp to key",
	Callback = function()
-- ตรวจสอบว่าโฟลเดอร์ CageKeyLocations มีอยู่หรือไม่ใน Workspace
local folder = workspace:FindFirstChild("CageKeyLocations")

if folder and folder:IsA("Folder") then
    -- เก็บ Tool ชื่อ CageKey ทั้งหมด
    local cageKeys = {}

    -- วนลูปผ่านวัตถุทั้งหมดในโฟลเดอร์และหาวัตถุที่เป็น Tool และมีชื่อว่า CageKey
    for _, item in pairs(folder:GetChildren()) do
        if item:IsA("Tool") and item.Name == "CageKey" then
            table.insert(cageKeys, item)
        end
    end

    -- ถ้าเจอ CageKey อย่างน้อย 1 อัน
    if #cageKeys > 0 then
        for _, cageKey in pairs(cageKeys) do
            -- หาตำแหน่งของ CageKey
            local cageKeyPosition = cageKey.Handle.Position -- สมมติว่า Tool มี Handle

            -- คำนวณตำแหน่งวาป
            local targetCFrame = CFrame.new(cageKeyPosition) * CFrame.new(0, 5, -5)

            -- วาปผู้เล่นไปตำแหน่งนั้น (สมมติว่ามีผู้เล่นท้องถิ่นที่เราต้องการวาป)
            local player = game.Players.LocalPlayer
            if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                player.Character.HumanoidRootPart.CFrame = targetCFrame
                wait(5) -- รอ 2 วินาทีก่อนจะวาปไปหาอันถัดไป
            end
        end
    else
        print("ไม่พบ Tool ที่ชื่อว่า CageKey")
    end
else
    print("ไม่พบโฟลเดอร์ CageKeyLocations ใน Workspace")
            end
  	end  
	})

