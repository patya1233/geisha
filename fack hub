fackTab:AddButton({
	Name = "tp to axe",
	Callback = function()
-- ตรวจสอบว่าโฟลเดอร์ AxePosition มีอยู่ใน Workspace หรือไม่
local axePositionFolder = game.Workspace:FindFirstChild("AxePosition")

if axePositionFolder then
    -- ตรวจสอบว่ามี Tool ชื่อ Axe อยู่ในโฟลเดอร์ AxePosition หรือไม่
    local axeTool = axePositionFolder:FindFirstChild("Axe")
    
    if axeTool and axeTool:IsA("Tool") then
        -- ตรวจสอบว่ามี Handle อยู่ใน Tool ชื่อ Axe หรือไม่
        local handle = axeTool:FindFirstChild("Handle")
        
        if handle then
            print("พบ Handle ใน Tool ชื่อ Axe")

            -- รับตัวละครของผู้เล่น
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()

            -- คำนวณตำแหน่งใหม่ โดยเพิ่ม 5 หน่วยในแกน Y
            local newPosition = handle.CFrame + Vector3.new(0, 5, 0)

            -- วาปตัวละครไปยังตำแหน่งที่คำนวณ
            character:SetPrimaryPartCFrame(newPosition)
        else
            print("ไม่พบ Handle ใน Tool ชื่อ Axe")
        end
    else
        print("ไม่พบ Tool ชื่อ Axe ในโฟลเดอร์ AxePosition")
    end
else
    print("ไม่พบโฟลเดอร์ AxePosition ใน Workspace")
end
        end
        })

