local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

local function teleportToPosition(position)
    humanoidRootPart.CFrame = position
    -- ทำให้กล้องหันลงไปที่พื้น โดยปรับมุม Pitch ลงไปที่ -90 องศา (หันลง)
    local cameraPosition = workspace.CurrentCamera.CFrame.Position
    local lookDownPosition = cameraPosition + Vector3.new(0, -1, 0) -- กล้องหันลง
    workspace.CurrentCamera.CFrame = CFrame.new(cameraPosition, lookDownPosition)
end

for _, position in ipairs(teleportPositions) do
    teleportToPosition(position)
    wait(0.5)  -- รอ 1 วินาทีก่อนวาร์ปไปตำแหน่งต่อไป
end
        end  
	})

