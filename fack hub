    CFrame.new(-285.89035, 7.24921989, 941.409241, -0.999997437, 8.26647053e-08, 0.0022661488, 8.25150011e-08, 1, -6.61547688e-08, -0.0022661488, -6.59676118e-08, -0.999997437)
