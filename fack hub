game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-246.528229, 12.9980259, -556.101807, -0.0216634255, 1.57485704e-08, -0.999765337, -2.87889232e-08, 1, 1.63760809e-08, 0.999765337, 2.91369293e-08, -0.0216634255)
        end  
	})

local Section = fackTab:AddSection({
	Name = "esp sickle"
})

fackTab:AddButton({
	Name = "esp sickle",
	Callback = function()
-- ฟังก์ชันตรวจสอบไฟล์ใน Workspace และจัดการ Highlights และ ESP Text
local function checkForSickles()
    for i = 1, 5 do
        local sickleName = "Sickle" .. i
        local sickle = workspace:FindFirstChild(sickleName)
        
        if sickle then
            print(sickleName .. " exists in Workspace.")
            
            -- ตรวจสอบว่ามี Highlights อยู่หรือไม่
            local existingHighlight = sickle:FindFirstChildOfClass("Highlight")
            if existingHighlight then
                -- ลบ Highlight อันเดิม
                existingHighlight:Destroy()
                print("Removed old highlight from " .. sickleName)
            end
            
            -- สร้างและเพิ่ม Highlight ใหม่
            local highlight = Instance.new("Highlight")
            highlight.Parent = sickle
            highlight.FillTransparency = 1 -- ทำให้ข้างในโปร่งใส
            highlight.OutlineColor = Color3.fromRGB(0, 255, 0) -- สีเขียว
            highlight.OutlineTransparency = 0 -- เส้นขอบชัดเจน
            
            print("Added new highlight to " .. sickleName)

            -- ตรวจสอบว่ามี BillboardGui อยู่แล้วหรือไม่ (ESP Text)
            local existingBillboardGui = sickle:FindFirstChildOfClass("BillboardGui")
            if existingBillboardGui then
                -- ลบ BillboardGui อันเดิม
                existingBillboardGui:Destroy()
                print("Removed old ESP text from " .. sickleName)
            end

            -- สร้างและเพิ่ม BillboardGui สำหรับ ESP Text
            local billboardGui = Instance.new("BillboardGui")
            billboardGui.Parent = sickle
            billboardGui.Adornee = sickle
            billboardGui.Size = UDim2.new(0, 50, 0, 50) -- ขนาดของกล่องข้อความ
            billboardGui.StudsOffset = Vector3.new(0, 3, 0) -- ระยะห่างจากวัตถุ (ยกสูง)
            billboardGui.AlwaysOnTop = true -- ให้ข้อความอยู่บนสุดเสมอ

            -- สร้าง TextLabel สำหรับแสดงชื่อไฟล์
            local textLabel = Instance.new("TextLabel")
            textLabel.Parent = billboardGui
            textLabel.Size = UDim2.new(1, 0, 1, 0) -- ขนาดของ TextLabel
            textLabel.BackgroundTransparency = 1 -- พื้นหลังโปร่งใส
            textLabel.Text = sickleName -- แสดงชื่อไฟล์
            textLabel.TextColor3 = Color3.new(0, 1, 0) -- สีขาว
            textLabel.TextScaled = true -- ขยายข้อความให้พอดีกับกล่อง
            
            print("Added ESP text to " .. sickleName)
        else
            print(sickleName .. " does not exist in Workspace.")
        end
    end
end

-- เรียกใช้ฟังก์ชัน
checkForSickles()
        end  
	})

local Section = fackTab:AddSection({
	Name = "esp jorogumo"
})

fackTab:AddButton({
	Name = "esp jorogumo",
	Callback = function()
local workspace = game:GetService("Workspace")
local jorogumoAI = workspace:FindFirstChild("JorogumoAI")

if jorogumoAI then
    local dummy = jorogumoAI:FindFirstChild("Dummy")
    
    if dummy then
        local jorogumo = dummy:FindFirstChild("Jorogumo")
        
        if jorogumo then
            -- ลบ Highlight เดิมถ้ามี
            local existingHighlight = jorogumo:FindFirstChildOfClass("Highlight")
            if existingHighlight then
                existingHighlight:Destroy()
            end

            -- เพิ่ม Highlight ใหม่
            local highlight = Instance.new("Highlight")
            highlight.Parent = jorogumo
            highlight.Adornee = jorogumo
            highlight.OutlineColor = Color3.fromRGB(255, 0, 0) -- สีเขียว
            highlight.FillColor = Color3.fromRGB(255, 0, 0) -- สีเขียว
            highlight.Transparency = 0.5 -- ปรับความโปร่งใสได้ที่นี่

            print("เพิ่ม Highlight สีเขียวให้กับ Jorogumo")
        else
            print("ไม่มีโมเดลชื่อ Jorogumo อยู่ใน Dummy")
        end
    else
        print("ไม่พบโมเดล Dummy ใน JorogumoAI")
    end
else
    print("ไม่พบโฟลเดอร์ JorogumoAI ใน Workspace")
end
        end  
	})

fackTab:AddButton({
	Name = "esp Generators and Closets",
	Callback = function()
-- รับโฟลเดอร์ Generators จาก Workspace
local generatorsFolder = game.Workspace:FindFirstChild("Generators")

-- ตรวจสอบว่าโฟลเดอร์ Generators มีอยู่หรือไม่
if generatorsFolder then
    -- วน loop ตรวจสอบ item ในโฟลเดอร์
    for _, item in ipairs(generatorsFolder:GetChildren()) do
        if item.Name == "Generator" and item:IsA("Model") then
            -- ตรวจสอบและลบ Highlight ที่มีอยู่แล้ว
            local existingHighlight = item:FindFirstChildOfClass("Highlight")
            if existingHighlight then
                existingHighlight:Destroy()
            end
            
            -- สร้าง Highlight ใหม่
            local highlight = Instance.new("Highlight")
            highlight.Parent = item
            highlight.FillColor = Color3.fromRGB(0, 255, 0) -- สีเขียว
            highlight.FillTransparency = 0.5 -- ปรับความโปร่งใส่
            highlight.OutlineTransparency = 0 -- ความโปร่งใสของขอบ
        end
    end
else
    print("Generators folder not found in Workspace.")
end

-- ตรวจสอบว่าโฟลเดอร์ชื่อ Closets อยู่ใน Workspace หรือไม่
local closetsFolder = game.Workspace:FindFirstChild("Closets")

if closetsFolder and closetsFolder:IsA("Folder") then
    -- วนลูปผ่านไฟล์ทั้งหมดในโฟลเดอร์
    for _, item in ipairs(closetsFolder:GetChildren()) do
        -- ตรวจสอบว่าไฟล์เป็น Model และมีชื่อว่า Closet หรือไม่
        if item:IsA("Model") and string.match(item.Name, "Closet") then
            -- ตรวจสอบว่ามี Highlights อยู่ใน Model หรือไม่ ถ้ามีให้ลบทิ้ง
            local existingHighlight = item:FindFirstChildOfClass("Highlight")
            if existingHighlight then
                existingHighlight:Destroy()
            end

            -- สร้าง Highlights ใหม่
            local highlight = Instance.new("Highlight")
            highlight.Name = "Highlight"
            highlight.Adornee = item  -- ตั้งให้ Highlight ทำงานกับ Model นี้
            highlight.Parent = item  -- ตั้ง Parent ให้เป็น Model
            highlight.FillColor = Color3.fromRGB(128, 0, 128)  -- สีด้านในโปร่งใส
            highlight.FillTransparency = 0.5  -- ตั้งค่าความโปร่งใสเป็น 1 (เต็ม)
            highlight.OutlineColor = Color3.fromRGB(128, 0, 128)  -- ตั้งสีขอบเป็นสีม่วง
            highlight.OutlineTransparency = 0  -- ตั้งความโปร่งใสของเส้นขอบเป็น 0 (ไม่โปร่งใส)

            print("เพิ่ม Highlights ให้กับ Model: " .. item.Name)
        end
    end
else
    print("ไม่พบโฟลเดอร์ชื่อ 'Closets' ใน Workspace")
end
        end  
	})

local Section = fackTab:AddSection({
	Name = "esp KeraOna"
})

fackTab:AddButton({
	Name = "esp KeraOna",
	Callback = function()
local aiFolder = game.Workspace:FindFirstChild("AI")

if aiFolder then
    local keraOnaModel = aiFolder:FindFirstChild("KeraOna")
    
    if keraOnaModel then
        -- ลบ Highlights อันเดิมถ้ามี
        local existingHighlight = keraOnaModel:FindFirstChildOfClass("Highlight")
        if existingHighlight then
            existingHighlight:Destroy()
        end
        
        -- สร้าง Highlights ใหม่
        local newHighlight = Instance.new("Highlight")
        newHighlight.Parent = keraOnaModel
        newHighlight.FillColor = Color3.fromRGB(255, 0, 0) -- สีแดง
        newHighlight.FillTransparency = 0.5 -- โปร่งใส 50%
        newHighlight.OutlineColor = Color3.fromRGB(255, 0, 0) -- สีแดง
        newHighlight.OutlineTransparency = 0 -- ไม่มีความโปร่งใส
        newHighlight.Adornee = keraOnaModel
    else
        print("ไม่พบโมเดล KeraOna ในโฟลเดอร์ AI")
    end
else
    print("ไม่พบโฟลเดอร์ AI ใน Workspace")
end
        end  
	})

fackTab:AddButton({
	Name = "esp Key parts",
	Callback = function()
local keySpawnFolder = game.Workspace:FindFirstChild("KeySpawn")

if keySpawnFolder then
    -- ฟังก์ชันสำหรับสร้าง ESP Text
    local function createESPText(part)
        local espText = Instance.new("BillboardGui")
        espText.Adornee = part
        espText.Size = UDim2.new(0, 50, 0, 50)
        espText.AlwaysOnTop = true
        espText.ExtentsOffset = Vector3.new(0, 2, 0) -- ปรับตำแหน่งของข้อความ

        local textLabel = Instance.new("TextLabel")
        textLabel.Size = UDim2.new(0, 50, 0, 50)
        textLabel.BackgroundTransparency = 1
        textLabel.TextColor3 = Color3.fromRGB(0, 255, 0) -- สีเขียว
        textLabel.TextScaled = true
        textLabel.Text = part.Name -- ตั้งชื่อเป็นชื่อของ part

        textLabel.Parent = espText
        espText.Parent = part
    end

    -- ตรวจสอบใน Parts "Part1" ถึง "Part9"
    for i = 1, 9 do
        local partName = "Part" .. i
        local part = keySpawnFolder:FindFirstChild(partName)
        
        if part then
            print("พบ " .. partName .. " ในโฟลเดอร์ KeySpawn")
            -- ตรวจสอบว่าใน Part นั้นมี KeyPart1 ถึง KeyPart9 อยู่หรือไม่
            for j = 1, 9 do
                local keyPartName = "KeyPart" .. j
                local keyPart = part:FindFirstChild(keyPartName)

                if keyPart then
                    print("พบ " .. keyPartName .. " ใน " .. partName)

                    -- ลบ Highlights อันเดิมถ้ามี
                    local existingHighlight = keyPart:FindFirstChildOfClass("Highlight")
                    if existingHighlight then
                        existingHighlight:Destroy()
                    end
                    
                    -- สร้าง Highlights ใหม่
                    local newHighlight = Instance.new("Highlight")
                    newHighlight.Parent = keyPart
                    newHighlight.FillColor = Color3.fromRGB(0, 255, 0) -- สีเขียว
                    newHighlight.FillTransparency = 0.5 -- โปร่งใส 50%
                    newHighlight.OutlineColor = Color3.fromRGB(0, 255, 0) -- สีเขียว
                    newHighlight.OutlineTransparency = 0 -- ไม่มีความโปร่งใส
                    newHighlight.Adornee = keyPart

                    -- ตรวจสอบและลบ ESP Text อันเก่า
                    local existingESP = keyPart:FindFirstChildOfClass("BillboardGui")
                    if existingESP then
                        existingESP:Destroy()
                    end

                    -- เพิ่ม ESP Text ใหม่
                    createESPText(keyPart)
                else
                    print("ไม่พบ " .. keyPartName .. " ใน " .. partName)
                end
            end
        else
            print("ไม่พบ " .. partName .. " ในโฟลเดอร์ KeySpawn")
        end
    end

    -- ตรวจสอบใน Part ชื่อ "Part"
    local mainPart = keySpawnFolder:FindFirstChild("Part")
    if mainPart then
        print("พบ Part ชื่อ 'Part' ในโฟลเดอร์ KeySpawn")
        -- ตรวจสอบว่าใน Part นั้นมี KeyPart1 ถึง KeyPart9 อยู่หรือไม่
        for j = 1, 9 do
            local keyPartName = "KeyPart" .. j
            local keyPart = mainPart:FindFirstChild(keyPartName)

            if keyPart then
                print("พบ " .. keyPartName .. " ใน Part ชื่อ 'Part'")

                -- ลบ Highlights อันเดิมถ้ามี
                local existingHighlight = keyPart:FindFirstChildOfClass("Highlight")
                if existingHighlight then
                    existingHighlight:Destroy()
                end
                
                -- สร้าง Highlights ใหม่
                local newHighlight = Instance.new("Highlight")
                newHighlight.Parent = keyPart
                newHighlight.FillColor = Color3.fromRGB(0, 255, 0) -- สีเขียว
                newHighlight.FillTransparency = 0.5 -- โปร่งใส 50%
                newHighlight.OutlineColor = Color3.fromRGB(0, 255, 0) -- สีเขียว
                newHighlight.OutlineTransparency = 0 -- ไม่มีความโปร่งใส
                newHighlight.Adornee = keyPart

                -- ตรวจสอบและลบ ESP Text อันเก่า
                local existingESP = keyPart:FindFirstChildOfClass("BillboardGui")
                if existingESP then
                    existingESP:Destroy()
                end

                -- เพิ่ม ESP Text ใหม่
                createESPText(keyPart)
            else
                print("ไม่พบ " .. keyPartName .. " ใน Part ชื่อ 'Part'")
            end
        end
    else
        print("ไม่พบ Part ชื่อ 'Part' ในโฟลเดอร์ KeySpawn")
    end
else
    print("ไม่พบโฟลเดอร์ KeySpawn ใน Workspace")
end
        end  
	})

OrionLib:Init()
