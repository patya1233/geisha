	Name = "esp fish and Wooden board",
	Callback = function()
-- ฟังก์ชันสำหรับการเพิ่ม Highlight สีแดงให้กับ Humanoid พร้อมโปร่งใส่
local function addHighlightToHumanoid(model)
    -- ค้นหา Humanoid ในโมเดล
    local humanoid = model:FindFirstChildOfClass("Humanoid")
    
    if humanoid then
        -- สร้าง Highlight
        local highlight = Instance.new("Highlight")
        highlight.Parent = model
        highlight.FillColor = Color3.new(1, 0, 0) -- สีแดง (RGB: 1, 0, 0)
        highlight.FillTransparency = 0.5 -- ปรับความโปร่งใส่ (0 = ทึบ, 1 = โปร่งใส่เต็มที่)
        
        print("เพิ่ม Highlight ให้กับ Humanoid ใน " .. model.Name)
    else
        print("ไม่พบ Humanoid ใน " .. model.Name)
    end
end

-- ตรวจสอบว่า Fish1 มีหรือไม่
local fish1 = workspace:FindFirstChild("Fish1")
if fish1 then
    addHighlightToHumanoid(fish1)
else
    print("ไม่พบ Fish1 ใน Workspace")
end

-- ตรวจสอบว่า Fish2 มีหรือไม่
local fish2 = workspace:FindFirstChild("Fish2")
if fish2 then
    addHighlightToHumanoid(fish2)
else
    print("ไม่พบ Fish2 ใน Workspace")
end

-- ตรวจสอบว่าใน Workspace มีโฟลเดอร์ GAME หรือไม่
local gameFolder = workspace:FindFirstChild("GAME")

if gameFolder then
    print("พบโฟลเดอร์ 'GAME'")
    
    -- ตรวจสอบว่าในโฟลเดอร์ GAME มีโฟลเดอร์ Bridge หรือไม่
    local bridgeFolder = gameFolder:FindFirstChild("Bridge")
    
    if bridgeFolder then
        print("พบโฟลเดอร์ 'Bridge'")
        
        -- ตรวจสอบว่าในโฟลเดอร์ Bridge มีโฟลเดอร์ Missing_Planks หรือไม่
        local missingPlanksFolder = bridgeFolder:FindFirstChild("Missing_Planks")
        
        if missingPlanksFolder then
            print("พบโฟลเดอร์ 'Missing_Planks'")
            
            -- ตรวจสอบว่าในโฟลเดอร์ Missing_Planks มี MeshPart บ้างหรือไม่
            local meshPartCount = 0
            for _, item in ipairs(missingPlanksFolder:GetChildren()) do
                if item:IsA("MeshPart") then
                    meshPartCount = meshPartCount + 1
                    print("พบ MeshPart: " .. item.Name)

                    -- เพิ่ม Highlight ให้กับ MeshPart
                    local highlight = Instance.new("Highlight")
                    highlight.Adornee = item
                    highlight.FillTransparency = 1 -- ทำให้ข้างในโปร่งใส
                    highlight.OutlineColor = Color3.fromRGB(0, 255, 0) -- สีเขียว
                    highlight.Parent = item
                end
            end
            
            if meshPartCount == 0 then
                print("ไม่พบ MeshPart ใน 'Missing_Planks'")
            else
                print("พบทั้งหมด " .. meshPartCount .. " MeshPart ใน 'Missing_Planks'")
            end
        else
            print("ไม่พบโฟลเดอร์ 'Missing_Planks'")
        end
    else
        print("ไม่พบโฟลเดอร์ 'Bridge'")
    end
else
    print("ไม่พบโฟลเดอร์ 'GAME'")
end
        end  
	})

