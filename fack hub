-- ฟังก์ชันตรวจสอบระยะห่างระหว่างผู้เล่นกับ MeshPart
local function checkPlayerProximity(meshPart, proximityPrompt)
    -- ระยะที่ผู้เล่นจะต้องเข้าใกล้เพื่อให้ ProximityPrompt ทำงาน
    local activationDistance = proximityPrompt.MaxActivationDistance or 10

    -- รับตำแหน่งของ MeshPart
    local partPosition = meshPart.Position

    -- ใช้การตรวจสอบทุกๆ 0.1 วินาที
    game:GetService("RunService").Heartbeat:Connect(function()
        -- วนผ่านผู้เล่นทุกคนในเกม
        for _, player in pairs(game.Players:GetPlayers()) do
            -- ตรวจสอบว่าผู้เล่นมีตัวละคร (Character) และหัวของตัวละครหรือไม่
            if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                -- คำนวณระยะห่างระหว่างผู้เล่นกับ MeshPart
                local distance = (player.Character.HumanoidRootPart.Position - partPosition).Magnitude

                -- ถ้าผู้เล่นอยู่ในระยะห่างที่กำหนด ให้ทำงาน ProximityPrompt
                if distance <= activationDistance then
                    print(player.Name .. " อยู่ใกล้ ProximityPrompt ใน " .. meshPart.Name)
                    proximityPrompt:InputHoldBegin(player)  -- จำลองการเริ่มใช้งาน
                    proximityPrompt:InputHoldEnd()          -- จำลองการใช้งานเสร็จสิ้น
                end
            end
        end
    end)
end

-- ถ้าเจอโฟลเดอร์ Missing_Planks
if folder then
    local meshParts = {} -- ตารางเก็บ MeshPart ทั้งหมด
    -- ลูปผ่านทุก MeshPart ในโฟลเดอร์
    for _, object in pairs(folder:GetChildren()) do
        -- ตรวจสอบว่า object เป็น MeshPart หรือไม่
        if object:IsA("MeshPart") then
            table.insert(meshParts, object) -- เก็บ MeshPart ไว้ในตาราง
            -- พิมพ์ชื่อและชนิดของ object
            print("ชื่อไฟล์: " .. object.Name)
            print("ชนิดไฟล์: " .. object.ClassName)
        end
    end
    
    -- ตรวจสอบว่าเจอ MeshPart 5 ไฟล์หรือไม่
    if #meshParts == 5 then
        -- ตรวจสอบ ProximityPrompt ใน MeshPart ทั้ง 5
        for _, meshPart in pairs(meshParts) do
            print("ตรวจสอบ ProximityPrompt ใน: " .. meshPart.Name)
            -- ลูปผ่าน children ของ MeshPart นั้น
            for _, child in pairs(meshPart:GetChildren()) do
                -- ถ้า child เป็น ProximityPrompt
                if child:IsA("ProximityPrompt") then
                    print("พบ ProximityPrompt ใน: " .. meshPart.Name)
                    -- เรียกฟังก์ชันตรวจสอบระยะห่าง
                    checkPlayerProximity(meshPart, child)
                end
            end
        end
    else
        print("ไม่พบ MeshPart จำนวน 5 ไฟล์ พบจำนวน: " .. #meshParts)
    end
else
    print("ไม่พบโฟลเดอร์ Missing_Planks")
end

local teleportPositions = {
    CFrame.new(-332.582214, -9.32113171, 845.324097, 0.748804569, -0.00512214098, -0.662771046, 4.38541174e-05, 0.999970496, -0.0076785963, 0.662790835, 0.00572070293, 0.748782694),
