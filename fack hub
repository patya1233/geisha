            if math.abs(relativePos.X) <= blockSizeX / 2 and
               math.abs(relativePos.Y) <= blockSizeY / 2 and
               math.abs(relativePos.Z) <= blockSizeZ / 2 then
                playersInPart = true
                break
            end
        end
    end
    return playersInPart
end

-- ลูปตรวจสอบอย่างต่อเนื่อง
while true do
    wait(1) -- หน่วงเวลา 1 วินาทีก่อนตรวจสอบอีกครั้ง
    
    -- ตรวจสอบว่ามีผู้เล่นอยู่บน Part หรือไม่
    if not checkPlayersInPart() then
        block:Destroy() -- ลบ Part ถ้าไม่มีผู้เล่นอยู่บน Part
        break -- หยุดลูปเมื่อ Part ถูกลบแล้ว
    end
end
        end  
	})

