fackTab:AddButton({
	Name = "esp Paint and Drawer",
	Callback = function()
-- ตรวจสอบว่าโฟลเดอร์ Mansion และ Drawers มีอยู่หรือไม่
local mansionFolder = game.Workspace:FindFirstChild("Mansion")
if mansionFolder then
    local drawersFolder = mansionFolder:FindFirstChild("Drawers")
    
    if drawersFolder then
        print("สิ่งที่อยู่ในโฟลเดอร์ Drawers:")
        
        -- วนลูปเพื่อตรวจสอบว่าในโฟลเดอร์ Drawers มีอะไรบ้าง
        local drawerNumber = 1
        
        for _, drawer in pairs(drawersFolder:GetChildren()) do
            if drawerNumber <= 7 then
                print(drawer.Name)
                
                -- ถ้ามี Highlight อยู่แล้ว ให้ลบทิ้งก่อน
                local existingHighlight = drawer:FindFirstChildOfClass("Highlight")
                if existingHighlight then
                    existingHighlight:Destroy()
                end
                
                -- เพิ่ม Highlight ใหม่
                local highlight = Instance.new("Highlight")
                highlight.Parent = drawer
                highlight.FillColor = Color3.fromRGB(0, 255, 0)  -- สีเขียวสำหรับด้านใน
                highlight.OutlineColor = Color3.fromRGB(0, 255, 0) -- สีเขียวสำหรับเส้นรอบ

                -- ถ้ามี BillboardGui (ESP Text) อยู่แล้ว ให้ลบทิ้งก่อน
                local existingBillboardGui = drawer:FindFirstChildOfClass("BillboardGui")
                if existingBillboardGui then
                    existingBillboardGui:Destroy()
                end

                -- สร้าง BillboardGui สำหรับ ESP Text
                local billboardGui = Instance.new("BillboardGui")
                billboardGui.Parent = drawer
                billboardGui.AlwaysOnTop = true
                billboardGui.Size = UDim2.new(0, 17, 0, 17) -- ขนาดของกล่องข้อความ
                billboardGui.StudsOffset = Vector3.new(0, 0, 0) -- ระยะห่างจากวัตถุ

                -- สร้าง TextLabel สำหรับตัวเลข ESP
                local textLabel = Instance.new("TextLabel")
                textLabel.Parent = billboardGui
                textLabel.Size = UDim2.new(1, 0, 1, 0) -- ขนาดข้อความเต็มกล่อง
                textLabel.BackgroundTransparency = 1 -- พื้นหลังโปร่งใส
                textLabel.Text = tostring(drawerNumber) -- แสดงตัวเลขเรียงจาก 1 ถึง 7
                textLabel.TextColor3 = Color3.fromRGB(255, 255, 255) -- สีฟ้าของตัวอักษร
                textLabel.TextScaled = true -- ขนาดข้อความปรับตามขนาดกล่อง

                drawerNumber = drawerNumber + 1
            end
        end
    else
        print("ไม่พบโฟลเดอร์ Drawers ใน Mansion")
    end
else
    print("ไม่พบโฟลเดอร์ Mansion ใน Workspace")
end

-- สคริปต์นี้จะตรวจสอบและเพิ่ม Highlights และ ESP Text ให้กับ Paint1 ถึง Paint9 ใน MoveablePaintings
-- หากมี Highlight และ ESP Text อันเดิมอยู่แล้ว จะลบทิ้งก่อน

-- เข้าถึง Workspace
local workspace = game:GetService("Workspace")

-- เข้าถึง Mansion
local mansion = workspace:FindFirstChild("Mansion")

-- ตรวจสอบว่า Mansion มีอยู่ใน Workspace หรือไม่
if mansion then
    -- เข้าถึง MoveablePaintings
    local moveablePaintings = mansion:FindFirstChild("MoveablePaintings")

    -- ตรวจสอบว่า MoveablePaintings มีอยู่ใน Mansion หรือไม่
    if moveablePaintings then
        print("กำลังลบ Highlight และ ESP Text อันเดิม (ถ้ามี) และเพิ่มอันใหม่ให้กับ Paint1 ถึง Paint9:")

        -- วน loop ผ่าน Paint1 ถึง Paint9
        for i = 1, 9 do
            local paintingName = "Paint" .. i
            local painting = moveablePaintings:FindFirstChild(paintingName)

            -- ตรวจสอบว่าพบ Paint1 ถึง Paint9 หรือไม่
            if painting then
                -- ตรวจสอบและลบ Highlight เดิมถ้ามี
                local existingHighlight = painting:FindFirstChildOfClass("Highlight")
                if existingHighlight then
                    existingHighlight:Destroy()
                    print("ลบ Highlight เดิมจาก " .. paintingName)
                end

                -- ตรวจสอบและลบ BillboardGui (ESP Text) เดิมถ้ามี
                local existingBillboardGui = painting:FindFirstChildOfClass("BillboardGui")
                if existingBillboardGui then
                    existingBillboardGui:Destroy()
                    print("ลบ ESP Text เดิมจาก " .. paintingName)
                end

                -- สร้าง Highlight ใหม่
                local highlight = Instance.new("Highlight")
                highlight.Parent = painting
                highlight.FillColor = Color3.new(0.5, 0, 0.5) -- สีม่วง
                highlight.FillTransparency = 0.5 -- โปร่งใส
                highlight.OutlineColor = Color3.new(0.5, 0, 0.5) -- สีม่วงสำหรับเส้นขอบ
                highlight.OutlineTransparency = 0 -- เส้นขอบไม่โปร่งใส

                -- สร้าง BillboardGui สำหรับ ESP Text ใหม่
                local billboardGui = Instance.new("BillboardGui")
                billboardGui.Parent = painting
                billboardGui.Size = UDim2.new(0, 17, 0, 17) -- ขนาดของกล่องข้อความ
                billboardGui.StudsOffset = Vector3.new(-1.3, 0, 0) -- ตำแหน่งเลื่อนขึ้นจากวัตถุ
                billboardGui.AlwaysOnTop = true -- ให้ข้อความแสดงอยู่เสมอ

                -- สร้าง TextLabel สำหรับแสดงหมายเลขใหม่
                local textLabel = Instance.new("TextLabel")
                textLabel.Parent = billboardGui
                textLabel.Size = UDim2.new(1, 0, 1, 0) -- ขนาดของข้อความใน BillboardGui
                textLabel.BackgroundTransparency = 1 -- ทำให้พื้นหลังโปร่งใส
                textLabel.Text = tostring(i) -- แสดงตัวเลข 1 ถึง 9
                textLabel.TextColor3 = Color3.new(1, 1, 0) -- สีเหลือง
                textLabel.TextScaled = true -- ปรับขนาดข้อความตามกล่อง

                print("เพิ่ม Highlight และ ESP Text ให้กับ " .. paintingName)
            else
                print(paintingName .. " ไม่พบใน MoveablePaintings")
            end
        end
    else
        print("ไม่พบ MoveablePaintings ใน Mansion")
    end
else
    print("ไม่พบ Mansion ใน Workspace")
end
        end  
	})

